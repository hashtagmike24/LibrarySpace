<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.csusb.libraryspace" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>

        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>

        <activity
            android:name=".GroupActivity"
            android:label="@string/title_activity_group" >
        </activity>
        <activity
            android:name=".IndividualActivity"
            android:label="@string/title_activity_individual" >
        </activity>
        <activity
            android:name=".MultimediaActivity"
            android:label="@string/title_activity_multimedia" >
        </activity>
        <activity
            android:name=".ConditionsActivity"
            android:label="@string/title_activity_conditions" >
        </activity>
        <activity
            android:name=".BookingActivity"
            android:label="@string/title_activity_booking" >
        </activity>
    </application>

</manifest>
